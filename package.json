{
  "name": "parkingspace-reactnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/cli-platform-android": "^10.1.3",
    "@react-native-community/datetimepicker": "^6.7.5",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.4",
    "@react-navigation/native-stack": "^6.9.10",
    "@reduxjs/toolkit": "^1.9.2",
    "alert": "^5.1.4",
    "axios": "^1.3.3",
    "jest-cucumber": "^3.0.1",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-app-utils": "^0.2.1",
    "react-native-get-random-values": "^1.8.0",
    "react-native-modal-datetime-picker": "^14.0.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-redux": "^8.0.5",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.71.3",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.19.0",
    "jest": "^29.4.3",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.0.5",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "cacheDirectory": "./cache",
    "coveragePathIgnorePatterns": [
      "./app/utils/vendor"
    ],
    "testMatch": [
      "**/features/step_definitions/**/*.steps.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80
      }
    },
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native|react-clone-referenced-element|react-navigation|react-native-modal-datetime-picker|alert|uuid|jest-cucumber|react-redux|react-router-dom|@reduxjs/toolkit|)"
    ]
  }
}
